@model IEnumerable<AlfaPeople.KingKhalidFoundation.Data.Model.FollowUpProjectPlanRequest>

@{
    ViewBag.Title = Resources.General.FollowUpProjectPlanRequest;
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<!-- Content -->
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">

                        <div class="material-datatables">
                            <table id="datatables" class="table table-striped table-no-bordered table-hover" cellspacing="0"
                                   width="100%" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>
                                            @Resources.FollowUpProjectPlan.FollowUpName
                                        </th>
                                        <th>
                                            @Resources.FollowUpProjectPlan.WorkShop
                                        </th>
                                        <th>
                                            @Resources.FollowUpProjectPlan.CorporateName
                                        </th>
                                        <th>
                                            @Resources.FollowUpProjectPlan.CorporateProfile
                                        </th>
                                        <th>
                                            @Resources.FollowUpProjectPlan.Status
                                        </th>
                                        <th>
                                            @Resources.FollowUpProjectPlan.Deadline
                                        </th>

                                        <th>@Resources.General.Actions</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>

                                            <td>
                                                @Html.DisplayFor(modelItem => item.FollowUpProjectPlan.Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FollowUpProjectPlan.IncubationWorkshop.Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FrontendUser.CorporateApplicationForms.FirstOrDefault().Name)
                                            </td>
                                            <td>
                                                <button type="button" class="btn btn-default btn-link" data-toggle="modal" onclick="GetData('@item.FrontendUserId')" data-target='#@item.FrontendUser.AspNetUser.AspNetRoles.First().Name'>
                                                    @*rel="tooltip" data-placement="bottom">*@
                                                    <i class="material-icons">art_track</i>
                                                </button>
                                            </td>
                                            <td>
                                                @if (item.FollowUpProjectPlanStatus == AlfaPeople.KingKhalidFoundation.Data.Model.FollowUpProjectPlanStatus.Accept)
                                                {
                                                    <span class="badge badge-pill badge-success">
                                                        @Resources.FollowUpProjectPlan.Accepted
                                                    </span>
                                                }
                                                else if (item.FollowUpProjectPlanStatus == AlfaPeople.KingKhalidFoundation.Data.Model.FollowUpProjectPlanStatus.Reject)
                                                {
                                                    <span class="badge badge-pill badge-danger">
                                                        @Resources.FollowUpProjectPlan.Rejected
                                                    </span>
                                                }
                                                else if (item.FollowUpProjectPlanStatus == AlfaPeople.KingKhalidFoundation.Data.Model.FollowUpProjectPlanStatus.ProjectPlanUpdated)
                                                {
                                                    <span class="badge btn-warning">
                                                        @Resources.FollowUpProjectPlan.ProjectPlanUpdated
                                                    </span>
                                                }
                                                else if (item.FollowUpProjectPlanStatus == AlfaPeople.KingKhalidFoundation.Data.Model.FollowUpProjectPlanStatus.UpdateProjectPlan)
                                                {
                                                    <span class="badge btn-info">
                                                        @Resources.FollowUpProjectPlan.UpdateProjectPlan
                                                    </span>
                                                }
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FollowUpProjectPlan.Deadline)
                                            </td>
                                            <td>
                                                @if (item.FollowUpProjectPlanStatus == AlfaPeople.KingKhalidFoundation.Data.Model.FollowUpProjectPlanStatus.ProjectPlanUpdated)
                                                {
                                                    <a href="@Url.Action("Details", new {Id = item.FollowUpProjectPlanRequestId})">
                                                        <i class="material-icons">details</i>
                                                    </a>
                                                }
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!-- end content-->
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Content -->
