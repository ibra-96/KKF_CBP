<style>
    .ajs-message.ajs-custome {
        color: white;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }

    .ajs-message.ajs-custome2 {
        color: indianred;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }
</style>
<!-- Content -->
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- <div class="col-md-10 ml-auto mr-auto"> -->
            <div class="col-md-12">
                <div class="card">

                    <div class="card-body">
                        <div class="toolbar">
                            <!--        Here you can write extra buttons/actions for the toolbar
                                        -->

                        </div>
                        @{
                            int count = 1;
                        }
                        <div class="material-datatables">
                            <table id="datatables" class="table table-striped table-no-bordered table-hover" cellspacing="0"
                                   width="100%" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>
                                            @Resources.IncubationRequest.RequesterName
                                        </th>
                                        <th>
                                            @Resources.IncubationRequest.ProjectName


                                        </th>
                                        <th>
                                            @Resources.IncubationRequest.Status
                                        </th>
                                        <th>@Resources.General.View</th>
                                        <th>@Resources.General.Details</th>
                                        @*<th>Accept</th>
                                            <th>Reject</th>*@
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FrontendUser.CorporateApplicationForms.FirstOrDefault().Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IncubationAdvertising.Name)
                                            </td>
                                            <td>

                                                @if (item.IncubationProjectProposalStatu.NameEN == "Accepted")
                                                {
                                                    <span class="badge badge-pill badge-success">
                                                        @Resources.IncubationRequest.Accepted
                                                    </span>
                                                }
                                                else if (item.IncubationProjectProposalStatu.NameEN == "Rejected")
                                                {
                                                    <span class="badge badge-pill badge-danger">
                                                        @Resources.IncubationRequest.Rejected
                                                    </span>
                                                }
                                                else
                                                {
                                                    <span class="badge btn-warning">
                                                        @if (ViewBag.lang == 3)
                                                        {
                                                            @(item.IncubationProjectProposalStatu.NameAR)
                                                        }
                                                        else
                                                        {
                                                            @(item.IncubationProjectProposalStatu.NameEN)
                                                        }
                                                    </span>
                                                }
                                            </td>
                                            <td>
                                                <button type="button" class="btn btn-default btn-link" data-toggle="modal" onclick="GetDataInd('@item.FrontendUserID','@item.FrontendUser.AspNetUser.AspNetRoles.First().Name')" data-target='#@item.FrontendUser.AspNetUser.AspNetRoles.First().Name'
                                                        rel="tooltip" data-placement="bottom">
                                                    <i class="material-icons">art_track</i>
                                                </button>
                                            </td>
                                            <td>
                                                @if (item.IncubationProjectProposalStatu.NameEN == "Pending" || item.IncubationProjectProposalStatu.NameEN == "Project Proposal Updated")
                                                {
                                                    <a href="@Url.Action("IncubationProjectProposalRequest",new { id = item.IncubationProjectProposalID, Type = ViewBag.Type })">
                                                        <i class="material-icons">details</i>
                                                    </a>
                                                }
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!-- end content-->
                </div>
                <!--  end card  -->
            </div>
            <!-- end col-md-12 -->
        </div>
        <!-- end row -->
    </div>
</div>
