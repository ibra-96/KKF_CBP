<!-- Content -->
<div class="content">
    <div class="content">
        <div class="container-fluid">
            <h3>Corporation Requests</h3>
            <br>
            <div class="row">

                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        <div id="@item.FrontendUserID" class="col-md-3">
                            <div class="card card-product">
                                <div class="card-header card-header-image" data-header-animation="true">
                                    <a href="#KKF">
                                        @{
                                            var base64 = Convert.ToBase64String(item.Picture);
                                            var imgSrc = String.Format("data:image/gif;base64,{0}", base64);
                                        }



                                        <img class="img" src="@imgSrc">

                                    </a>
                                </div>
                                <div class="card-body">
                                    <div class="card-actions text-center">
                                        <button type="button" class="btn btn-danger btn-link fix-broken-card">
                                            <i class="material-icons">build</i> Fix Header!
                                        </button>
                                        <button type="button" class="btn btn-default btn-link" data-toggle="modal" onclick="GetData('@item.FrontendUserID')" data-target="#AddEmployee">
                                            @*rel="tooltip" data-placement="bottom" title="View">*@
                                            <i class="material-icons">art_track</i>
                                        </button>
                                        <button type="button" onclick="Confirm('@item.FrontendUserID')" class="btn btn-success btn-link" rel="tooltip" data-placement="bottom"
                                                title="Confirm">
                                            <i class="material-icons">check</i>
                                        </button>
                                        <button type="button" onclick="bindUserId(this)" data-userid="@item.FrontendUserID" class="btn btn-danger btn-link" rel="tooltip" data-toggle="modal" data-target="#Rejectmodal" data-placement="bottom" title="Reject">
                                            <i class="material-icons">close</i>
                                        </button>
                                    </div>
                                    <h4 class="card-title">
                                        <a href="#KKF">@item.Name </a>
                                    </h4>
                                    <div class="card-description">
                                        @item.History
                                    </div>
                                </div>
                                <!-- <div class="card-footer">
                                  <div class="price">
                                    <h4>$899/night</h4>
                                  </div>
                                  <div class="stats">
                                    <p class="card-category">
                                      <i class="material-icons">place</i> Barcelona, Spain</p>
                                  </div>
                                </div> -->
                            </div>
                        </div>
                    }
                }
            </div>
        </div>
    </div>
</div>
<!-- End Content -->
