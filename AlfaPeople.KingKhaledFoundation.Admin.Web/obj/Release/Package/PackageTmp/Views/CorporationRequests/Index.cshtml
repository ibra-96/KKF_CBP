@model IEnumerable<AlfaPeople.KingKhalidFoundation.Data.Model.CorporateApplicationForm>
@{
    ViewBag.title = "Corporation Requests";
    ViewBag.Name = User.Identity.Name;
}

<style>
    .ajs-message.ajs-custome {
        color: white;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }

    .ajs-message.ajs-custome2 {
        color: indianred;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
