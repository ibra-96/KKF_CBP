            var msgConfirm = confirm("Are you sure about taken this action, This action will send an email to the corporates?");
            if (msgConfirm == true) {
                $.getJSON('@Url.Action("ConfirmCorUser", "Home")', { "FronUserID": FronUserID }, function (Result) {

                    if (Result != null && !jQuery.isEmptyObject(Result)) {
                        if (Result != "Error") {

                            alertify.notify(Result, 'custome', 4, function () { console.log('dismissed'); });

                            $("#" + FronUserID).remove();
                        }
                        else {
                            alertify.notify(Result, 'custome2', 4, function () { console.log('dismissed'); });
                        }
                    };
                });
            } else {
                //event.preventDefault();
            }
    }



        function ConfirmInd(FronUserID) {
