                if (msgConfirm == true) {
                    $.getJSON('@Url.Action("RejectCorUser", "Home")', { "FronUserID": FronUserID, "Feadback": feadback, "ReasonType": radioValue }, function (Result) {

                        if (Result != "Error") {
                            alertify.notify(Result, 'custome', 4, function () { console.log('dismissed'); });
                            $("#" + FronUserID).remove();
                        }
                        else
                        {
                            alertify.notify(Result, 'custome2', 4, function () { console.log('dismissed'); });
                        }
