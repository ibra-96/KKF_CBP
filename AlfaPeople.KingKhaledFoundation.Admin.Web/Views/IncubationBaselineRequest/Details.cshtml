                                        @*@Html.ValidationMessageFor(model => model.BoardDirectorRoles, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12" style="margin-bottom: 14px;margin-top: 14px;">
                                <span style="font-size: 14px;" class="badge badge-pill badge-rose">
                                    @Resources.IncubationRequest.FinanceResource
