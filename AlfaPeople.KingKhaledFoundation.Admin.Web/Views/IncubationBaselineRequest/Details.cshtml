                                        @*@Html.ValidationMessageFor(model => model.SupportersCountLastYear, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
                                <div class="col-sm-12" style="margin-bottom: 14px;margin-top: 14px;">
                                    <span style="font-size: 12px;" class="badge badge-pill badge-default">
                                        @Resources.IncubationRequest.NumberOfDonors
                                    </span>
                                </div>
                                <div class="row col-lg-12">
                                    <div class="col-lg-2">
                                        <div class="form-group">
                                            <label for="DonorInstitutions">
                                                @Html.CheckBoxFor(model => model.DonorInstitutions) @Resources.IncubationRequest.DonorFoundations
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-lg-2">
                                        <div class="form-group">
                                            <label for="GovernmentAgencies">
                                                @Html.CheckBoxFor(model => model.GovernmentAgencies) @Resources.IncubationRequest.GovernmentAgencies
