@model AlfaPeople.KingKhalidFoundation.Data.Model.IncubationBaseline
<link href="~/assets/css/FileUploaderStyle.css" rel="stylesheet" />
@{
    ViewBag.title = Resources.General.BaselineRequest; // "Project Proposal Request";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<style>
    .ajs-message.ajs-custome {
        color: white;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }

    .ajs-message.ajs-custome2 {
        color: indianred;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }

    /*.card-stats .card-header.card-header-text {text-align: left;}*/
</style>

<!-- Content -->
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="card">
                <div class="card-body ">
                    @using (Html.BeginForm("Details", "IncubationBaselineRequest", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                    {
                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(model => model.IncubationBaselineID)
                        @Html.HiddenFor(model => model.FrontendUserID)
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                        <div id="BodyContent">
                            <div class="col-sm-12" style="margin-bottom: 14px;margin-top: 14px;">
                                <span style="font-size: 14px;" class="badge badge-pill badge-rose">
                                    @Resources.IncubationRequest.TheStrategy
                                </span>
                            </div>
                            <div class="col-sm-4"></div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label for="ReasonEstablishingCorporate" class="bmd-label-floating">@Resources.IncubationRequest.OrganizationEstablishedFor </label>
                                        @Html.TextAreaFor(model => model.ReasonEstablishingCorporate, new { @class = "form-control" })
                                        @*@Html.ValidationMessageFor(model => model.ReasonEstablishingCorporate, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label for="History" class="bmd-label-floating"> @Resources.IncubationRequest.History </label>
                                        @Html.TextAreaFor(model => model.History, new { @class = "form-control" })
