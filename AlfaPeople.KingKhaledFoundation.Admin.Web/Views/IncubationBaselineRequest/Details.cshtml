                                    <div class="form-group">
                                        <label for="Vision" class="bmd-label-floating"> @Resources.IncubationRequest.Vision </label>
                                        @Html.TextAreaFor(model => model.Vision, new { @class = "form-control" })
                                        @*@Html.ValidationMessageFor(model => model.Vision, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label for="Mission" class="bmd-label-floating"> @Resources.IncubationRequest.Mission </label>
                                        @Html.TextAreaFor(model => model.Mission, new { @class = "form-control" })
                                        @*@Html.ValidationMessageFor(model => model.Mission, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label for="Objectives" class="bmd-label-floating"> @Resources.IncubationRequest.Objectives </label>
                                        @Html.TextAreaFor(model => model.Objectives, new { @class = "form-control" })
                                        @*@Html.ValidationMessageFor(model => model.Objectives, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
