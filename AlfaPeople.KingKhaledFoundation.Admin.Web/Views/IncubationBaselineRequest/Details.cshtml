                                            <i class="material-icons">delete</i> @Resources.IncubationRequest.Remove
                                        </a>
                                    </div>
                                    <div id="GovernanceGuide_UploadDiv">
                                        <label for="GovernanceGuide" class="bmd-label-floating"> @Resources.IncubationRequest.PleaseAttachAGovernmentGuide </label>
                                        @Html.TextBoxFor(model => model.GovernanceGuide, "", new { @type = "file" })*
                                        @*@Html.ValidationMessageFor(model => model.GovernanceGuide, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
                                <div class="col-lg-6">
