                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-12">
                                                                    <div class="form-group">
                                                                        <label for="IncubationBL_HowPlanMade" class="bmd-label-floating"> @Resources.IncubationRequest.HowTheStratgicPlanDeveloped </label>
                                                                        @Html.TextAreaFor(model => model.IncubationBL.HowPlanMade, new { @class = "form-control" })
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4" id="StrategicPlan_LinkDiv">
                                                                    <div>
                                                                        <label for="StrategicPlan" class="bmd-label-floating"> @Resources.IncubationRequest.PleaseAttachTheStrategicPlan </label>
                                                                        <a class="text-rose" href="@Url.Action("DownloadIncubBLAttachment", new { URL = Model.IncubationBL.StrategicPlan })">
                                                                            <i class="material-icons">get_app</i>
                                                                        </a>
                                                                        <a onclick="replaceAttachmentFile('@Model.IncubationBL.IncubationBaselineID', 'StrategicPlan', '@Model.IncubationBL.StrategicPlan')" class="text-danger remove" href="#" @*href="@Url.Action("DeleteIncubPPAttachment", new { Id = Model.IncubationBL.IncubationProjectProposalID, attach = "StrategicPlan", URL = Model.IncubationBL.StrategicPlan })"*@>
                                                                            <i class="material-icons">delete</i>
