    <script>
        function replaceAttachmentFile(IncubPPId, Attach, Url) {
            $.getJSON('@Url.Action("DeleteIncubBLAttachment")', { Id: IncubPPId, attach: Attach, URL: Url }, function (Result) {
                if (Result != null && !jQuery.isEmptyObject(Result)) {
                    if (Result === "Deleted Succsesfully") {
                        switch (Attach) {
                            case "StrategicPlan":
                                $("#StrategicPlan_LinkDiv").hide();
                                $("#StrategicPlan_UploadDiv").show();
                                break;
                            case "OperatingPlan":
                                $("#OperatingPlan_LinkDiv").hide();
                                $("#OperatingPlan_UploadDiv").show();
                                break;
                            case "GovernanceGuide":
                                $("#GovernanceGuide_LinkDiv").hide();
                                $("#GovernanceGuide_UploadDiv").show();
                                break;
                            case "CharteredAccountantNotes":
                                $("#CharteredAccountantNotes_LinkDiv").hide();
                                $("#CharteredAccountantNotes_UploadDiv").show();
                                break;
                            case "AttachFollowUpandEvaluationForms":
                                $("#AttachFollowUpandEvaluationForms_LinkDiv").hide();
                                $("#AttachFollowUpandEvaluationForms_UploadDiv").show();
                                break;
                        }
                    }
                }
            });
            return false;
        }

        $(document).ready(function () {
            var status = '@ViewBag.IncubBLStatus';
            if (status != '' && status != 'Draft' && status != 'Update Baseline Application Form') {
                $(".remove").hide();
                $("#saveBtn").hide();
                $("#BodyContent *").attr("disabled", "disabled").off('click');
            }
            else {
            }
        });

        $("#saveBtn").click(function () {
            $("#DraftFlage").val(true)
        });
    </script>

