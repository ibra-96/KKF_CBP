                                                                            <i class="material-icons">get_app</i>
                                                                        </a>
                                                                        <a onclick="replaceAttachmentFile('@Model.IncubationBL.IncubationBaselineID', 'OperatingPlan', '@Model.IncubationBL.StrategicPlan')" class="text-danger remove" href="#" @*href="@Url.Action("DeleteIncubPPAttachment", new { Id = Model.IncubationBL.IncubationProjectProposalID, attach = "OperatingPlan", URL = Model.IncubationBL.OperatingPlan })"*@>
                                                                            <i class="material-icons">delete</i>
                                                                        </a>
                                                                    </div>
                                                                    <div id="OperatingPlan_UploadDiv">
                                                                        <label for="OperatingPlan" class="bmd-label-floating"> @Resources.IncubationRequest.PleaseAttachThisYearOperatingPlan </label>
                                                                        @Html.TextBoxFor(model => model.OperatingPlan, "", new { @type = "file" })
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-12">
                                                                    <div class="form-group">
                                                                        <label for="IncubationBL_StrategicObjective" class="bmd-label-floating"> @Resources.IncubationRequest.DoesTheOrganztionHaveAnStratgicObject </label>
                                                                        @Html.TextAreaFor(model => model.IncubationBL.StrategicObjective, new { @class = "form-control" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-12" style="margin-bottom: 14px;margin-top: 14px;">
                                                                <span style="font-size: 14px;" class="badge badge-pill badge-rose">
                                                                    @Resources.IncubationRequest.BoardOfDirectors
                                                                </span>
                                                            </div>
                                                            <div class="col-sm-4"></div>
                                                            <div class="row">
                                                                <div class="col-sm-6">
                                                                    <div class="form-group">
