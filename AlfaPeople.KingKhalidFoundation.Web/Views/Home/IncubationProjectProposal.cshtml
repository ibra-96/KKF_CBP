                                                                        <a class="text-rose" href="@Url.Action("DownloadIncubBLAttachment", new { URL = Model.IncubationBL.GovernanceGuide })">
                                                                            <i class="material-icons">get_app</i>
                                                                        </a>
                                                                        <a onclick="replaceAttachmentFile('@Model.IncubationBL.IncubationBaselineID', 'GovernanceGuide', '@Model.IncubationBL.StrategicPlan')" class="text-danger remove" href="#" @*href="@Url.Action("DeleteIncubPPAttachment", new { Id = Model.IncubationBL.IncubationProjectProposalID, attach = "GovernanceGuide", URL = Model.IncubationBL.GovernanceGuide })"*@>
                                                                            <i class="material-icons">delete</i>
