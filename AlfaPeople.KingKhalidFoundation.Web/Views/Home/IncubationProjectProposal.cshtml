                                                                        @Html.TextBoxFor(model => model.StrategicPlan, "", new { @type = "file" })
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group">
                                                                        <label for="OperatingPlanCheckBox">
                                                                            @Html.CheckBox("OperatingPlanCheckBox")
