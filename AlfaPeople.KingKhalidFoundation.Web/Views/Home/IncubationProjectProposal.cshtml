                                                                        @Html.TextAreaFor(model => model.IncubationBL.ReasonEstablishingCorporate, new { @class = "form-control" })
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-12">
                                                                    <div class="form-group">
                                                                        <label for="IncubationBL_History" class="bmd-label-floating"> @Resources.IncubationRequest.History </label>
                                                                        @Html.TextAreaFor(model => model.IncubationBL.History, new { @class = "form-control" })
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-12">
                                                                    <div class="form-group">
                                                                        <label for="IncubationBL_Vision" class="bmd-label-floating"> @Resources.IncubationRequest.Vision </label>
                                                                        @Html.TextAreaFor(model => model.IncubationBL.Vision, new { @class = "form-control" })
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-12">
                                                                    <div class="form-group">
                                                                        <label for="IncubationBL_Mission" class="bmd-label-floating"> @Resources.IncubationRequest.Mission </label>
                                                                        @Html.TextAreaFor(model => model.IncubationBL.Mission, new { @class = "form-control" })
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-12">
                                                                    <div class="form-group">
