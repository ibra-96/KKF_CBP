                                                        @Html.HiddenFor(model => model.IncubationAd.EndDate)
                                                        @Html.HiddenFor(model => model.IncubationAd.AdvertisingDetails)
                                                        @Html.HiddenFor(model => model.IncubationAd.EntryRequirements)
                                                        @Html.HiddenFor(model => model.IncubationAd.IncubationTypeID)
                                                        @Html.HiddenFor(model => model.IncubationAd.FundingSourceID)
                                                        @Html.HiddenFor(model => model.IncubationAd.IsActive)
                                                        @Html.HiddenFor(model => model.IncubationAd.ISPublic)

                                                        @Html.HiddenFor(model => model.IncubationBL.FrontendUserID)
                                                        @Html.HiddenFor(model => model.IncubationBL.IncubationBaselineID)
                                                        @Html.HiddenFor(model => model.IncubationBL.IncubationBaselineStatusID)
                                                        @Html.HiddenFor(model => model.IncubationBL.submissionDate)
                                                        @Html.HiddenFor(model => model.IncubationBL.Feadback)
                                                        @Html.HiddenFor(model => model.IncubationBL.StrategicPlan)
                                                        @Html.HiddenFor(model => model.IncubationBL.OperatingPlan)
