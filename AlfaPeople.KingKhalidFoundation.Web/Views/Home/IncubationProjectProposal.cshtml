@model AlfaPeople.KingKhalidFoundation.Web.Models.IncubationProjectProposalVM
<link href="~/assets/css/FileUploaderStyle.css" rel="stylesheet" />
@{
    ViewBag.title = Resources.IncubationRequest.Baseline;
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<style>
    .ajs-message.ajs-custome {
        color: white;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }

    .ajs-message.ajs-custome2 {
        color: indianred;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }
</style>

<!-- Content -->
@*<div class="content">*@


<div class="content">
    <div class="container-fluid">



        <div class="row">

            <div class="card">
                @*<div class="card-header">
                        <h4 class="card-title">Collapsible Accordion</h4>
                    </div>*@
                <div class="card-body">
                    <div id="accordion" role="tablist">
                        <div class="card-collapse">
                            <div class="card-header" role="tab" id="headingOne">
                                <h5 class="mb-0">
                                    <a data-toggle="collapse" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne" class="collapsed">
                                        @Resources.IncubationRequest.IncubationAdvertisingDetails
                                        <i class="material-icons">keyboard_arrow_down</i>
                                    </a>
                                </h5>
                            </div>


                            <div id="collapseOne" class="collapse show" role="tabpanel" aria-labelledby="headingOne" data-parent="#accordion" style="">
                                <div class="card-body ">
                                    <div class="row justify-content-center">
                                        <div class="col-sm-6">
                                            <label for="incubationAdName" class="bmd-label-floating"> @Resources.IncubationRequest.AdvertisingName </label>
                                            <div class="form-group">
                                                <input type="text" id="incubationAdName" name="incubationAdName" value="@Model.IncubationAd.Name" class="form-control" disabled="disabled" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            @*<label for="incubationAdModels" class="bmd-label-floating"> @Resources.IncubationRequest.IncubationAdvertisingModels </label>
                                                <div class="form-group">
                                                    <select id="incubationAdModels" name="incubationAdModels" class="select-wizard form-control selectpicker" data-style="select-with-transition" disabled="disabled" multiple="multiple" title="Select Advertising Models">
                                                        @foreach (var item in @Model.IncubationAd.IncubationAdvertisingModels)
                                                        {
                                                            <option selected="selected" value="@item.IncubationModel.IncubationModelID">@item.IncubationModel.NameEN </option>
                                                        }
                                                    </select>
                                                </div>*@
                                        </div>
                                        <div class="col-sm-6">
                                            <label for="incubationAdStartDate" class="bmd-label-floating"> @Resources.IncubationRequest.StartDate </label>
                                            <div class="form-group">
                                                <input type="text" id="incubationAdStartDate" name="incubationAdStartDate" value="@Model.IncubationAd.StartDate.ToString(" yyyy-MM-dd")" class="form-control" disabled="disabled" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <label for="incubationAdEndDate" class="bmd-label-floating"> @Resources.IncubationRequest.EndDate </label>
                                            <div class="form-group">
                                                <input type="text" id="incubationAdEndDate" name="incubationAdEndDate" value="@Model.IncubationAd.EndDate.ToString(" yyyy-MM-dd")" class="form-control" disabled="disabled" />
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <label for="incubationAdDetails" class="bmd-label-floating">@Resources.IncubationRequest.AdvertisingDetails</label>
                                            <div class="form-group">
                                                <textarea id="incubationAdDetails" name="incubationAdDetails" class="form-control" rows="3" disabled="disabled">@Model.IncubationAd.AdvertisingDetails</textarea>
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <label for="incubationAdRequirements" class="bmd-label-floating">@Resources.IncubationRequest.EntryRequirements</label>
                                            <div class="form-group">
                                                <textarea id="incubationAdRequirements" name="incubationAdRequirements" class="form-control" rows="3" disabled="disabled">@Model.IncubationAd.EntryRequirements</textarea>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="material-datatables">
                                                <table id="AttachmentDatatables" class="table table-striped table-no-bordered table-hover" cellspacing="0" style="width:100%">
                                                    <thead>
                                                        <tr>
                                                            <th>@Resources.General.AttachmentName</th>
                                                            <th>@Resources.General.AttachmentType</th>
                                                            <th>@Resources.General.AttachmentSize</th>
                                                            <th class="disabled-sorting">@Resources.General.Actions</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (var item in Model.IncubationAd.IncubationAdvertisingAttachments)
                                                        {
                                                            <tr>
                                                                <td>
                                                                    <a class="btn btn-link btn-default" href="@Url.Action("DownloadIncubationAdvertisingAttachments", new { id=item.AttachmentID })">
                                                                        <i class="material-icons">attach_file</i>
                                                                        @item.Name
                                                                    </a>
                                                                </td>
                                                                <td>@item.Type</td>
                                                                <td>@item.Size Bytes</td>
                                                                <td class="text-right">
                                                                    <a class="btn btn-link btn-rose btn-just-icon" href="@Url.Action("DownloadIncubationAdvertisingAttachments", new { id=item.AttachmentID })">
                                                                        <i class="material-icons">get_app</i>
                                                                    </a>
                                                                </td>
                                                            </tr>
                                                        }
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>









                            <div class="card-collapse">
                                <div class="card-header" role="tab" id="headingTwo">
                                    <h5 class="mb-0">
                                        <a class="collapsed" data-toggle="collapse" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                            @Resources.IncubationRequest.Baseline
                                            <i class="material-icons">keyboard_arrow_down</i>
                                        </a>
                                    </h5>
                                </div>
                                <div id="collapseTwo" class="collapse" role="tabpanel" aria-labelledby="headingTwo" data-parent="#accordion">
                                    <div class="card-body ">
                                        <div class="card" @if (Model.IncubationAd.FundingSource.UseCustomThemes) { <text> style="background-image: url('@Model.IncubationAd.FundingSource.GrantBackgroundPic'); background-size: cover; background-repeat: round; background-position: center;" </text> }>
                                            <div class="card-header card-header-rose card-header-icon">
                                                @if (Model.IncubationAd.FundingSource.UseCustomThemes)
                                                {
                                                    <div class="card-icon" style="padding:0px; background: rgba(0, 0, 0, 0); background-color: rgba(0, 0, 0, 0); box-shadow: 0px 0px 0px 0px rgba(0, 0, 0, 0), 0px 0px 0px 0px rgba(0, 0, 0, 0);">
                                                        <img src="@Model.IncubationAd.FundingSource.GrantHeaderPic" style="width:101%; border-radius: 3px;" />
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="card-icon">
                                                        <i class="material-icons">all_inclusive</i>
                                                    </div>
                                                }
                                            </div>
                                            <div class="card-body">
