@model AlfaPeople.KingKhalidFoundation.Web.Models.CorporationRegistrationFormVM
@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<style>
    input[type=text] {
        background-color: white;
    }
</style>

<style>
    .ajs-message.ajs-custome {
        color: white;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }

    .ajs-message.ajs-custome2 {
        color: indianred;
        background-color: dimgrey;
        border-color: #31708f;
        text-align: center;
        font-size: medium;
    }
</style>
<div class="content">
    <div class="container-fluid">
        <div class="col-md-10 col-12 mr-auto ml-auto">
            <!--      Wizard container        -->
            <div class="wizard-container">
                <div class="card card-wizard" data-color="rose" id="wizardProfile">

                    @using (Html.BeginForm("CorporationProfile", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(f => f._RegisterVM.Username)
                        @Html.HiddenFor(f => f._RegisterVM._RegisterViewModel.ConfirmPassword)
