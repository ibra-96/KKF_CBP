                                                    @{
                                                        var imgSrc = (Model._CorporateApplicationForm.Picture != null && Model._CorporateApplicationForm.Picture.Length > 0)
                                                            ? $"data:image/png;base64,{Convert.ToBase64String(Model._CorporateApplicationForm.Picture)}"
                                                            : Url.Content("~/assets/img/default-avatar.png"); // الصورة الافتراضية
                                                    }

                                                    <img src="@imgSrc" name="_CorporateApplicationForm.picture" class="picture-src" id="wizardPicturePreview" />
                                                </div>

                                                @*<div class="picture">

            @{
                var base64 = Convert.ToBase64String(@Model._CorporateApplicationForm.Picture);
                var imgSrc = String.Format("data:image/gif;base64,{0}", base64);
            }


            <img src="@imgSrc" name="_CorporateApplicationForm.picture" class="picture-src" id="wizardPicturePreview" />
        </div>*@
                                            </div>
                                        </div>
